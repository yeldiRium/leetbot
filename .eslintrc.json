{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2019,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "node": true,
    "es2017": true
  },
  "rules": {
    "no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }]
  }
}
