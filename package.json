  "main": "src/index.js",
  "scripts": {
    "bot": "NODE_ENV=production node src/index.js",
    "bot:dev": "NODE_ENV=development nodemon src/index.js",
    "test": "yarn run test:lint && yarn run test:jest",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:lint": "prettier -c \"**/*.js\"",
    "test:lint:fix": "prettier --write \"**/*.js\"",
    "publish-patch": "yarn test && yarn version --patch && git push --tags && git push",
    "publish-minor": "yarn test && yarn version --minor && git push --tags && git push",
    "publish-major": "yarn test && yarn version --major && git push --tags && git push"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "dependencies": {
    "@yeldirium/redux-combine-getters": "^2.1.2",
    "@yeldirium/redux-migrations": "^1.2.0",
    "i18next": "^19.0.1",
    "moment-timezone": "^0.5.27",
    "node-schedule": "^1.3.1",
    "ramda": "^0.26.1",
    "redux": "^4.0.1",
    "telegraf": "^3.33.3"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "jest-extended": "^0.11.0",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "html",
      "lcov"
    ]
  }
}
